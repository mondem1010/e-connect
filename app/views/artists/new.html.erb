<div class="artist_new_space">
</div>
<div class="artist_new">
	<h1>アーティスト新規投稿</h1>
		<%= form_for(@artist) do |f| %>
   			<h4>タイトル</h4>
    		<%= f.text_field :artist_name %>
		    <h4>本文</h4>
    		<%= f.text_area :introduction %>
    		<h4>画像</h4>
    		<%= f.attachment_field :image %>
    		<%= f.submit '投稿' %>
		<% end %>
</div>